@import '/app.wxss';

#reset {
  display: block;
}

.tabs {
  position: fixed;
  top: 11%;
  width: 100%;
}

page {
  height: 100%;
  background-color: #fff;
}

/* 2.主盒子 */
.contain {
  margin-top: 10rpx;
  padding: 0rpx;
  width: 100%;
  /* 宽度占屏幕的100% */
  height: 89vh;
  /* 高度占屏幕的100% */
  background-color: #fff;
  /* 背景颜色 */

}

.nav_right_items {
  width: 100%;
  height: auto;
  /* 每个item设置高度是120px */
  background-color: #fff;
  text-align: center;
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  border-bottom: 1rpx solid rgb(221, 221, 221);
}

.head_image {
  width: 200rpx;
  height: 200rpx;
}

._image {
  display: block;
  width: 200rpx;
  height: 200rpx;
}

.text_name {
  display: -webkit-box;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 28rpx;
  line-height: 24rpx;
  margin-left: 20rpx;
  text-align: left;
}

.text_big {
  position: absolute;
  left: 247rpx;
  top: 92rpx;
  color: rgb(150, 150, 150);
  background-color: #d4d4d4;
  font-size: 22rpx;
  display: block;
  border-radius: 10rpx;
  width: 100rpx;
}

.text_price {
  position: absolute;
  top: 146rpx;
  left: 290rpx;
  color: #FF2B41;
  font-size: 36rpx;
}

.person_box {
  width: auto;
  position: absolute;
  top: 140rpx;
  left: 250rpx;
  display: flex;
}

.text_choose {
  color: #FF2B41;
  font-size: 22rpx;
}

.text_amount {
  position: absolute;
  top: 180rpx;
  left: 250rpx;
  font-size: 22rpx;
  color: rgb(150, 150, 150);
}


.look {
  display: flex;
  margin-left: 10rpx;
}

.look .peopleNums {
  display: block;
  font-size: 22rpx;
  color: #666666FF;

}

.place {
  color: #FF3742FF;
  font-size: 24rpx;
  position: absolute;
  top: 140rpx;
  left: 218rpx;
}

.place text {
  position: absolute;
  top: 0rpx;

}

.goToDetail {
  position: absolute;
  right: 30rpx;
  top: 130rpx;
}

.title {
  font-size: 26rpx;
  color: #aaa;
  padding: 20rpx 30rpx;
}

.offline_dis {
  width: 100%;
  padding: 26rpx 20rpx 0rpx;
  height: 77vh;
}

.boxs {
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-radius: 6rpx;
  display: flex;
  margin-bottom: 20rpx;
}

.left_box {
  background: linear-gradient(to right, #ff592d, #ff2e40);
  height: 100%;
  width: 180rpx;
  border-right: 10rpx dotted #fff;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
  border-radius: 6rpx;
}

.left_box text:first-child {
  font-size: 32rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.left_box text:last-child {
  font-size: 20rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.offline_dis_name {
  width: 80%;
  padding-left: 20rpx;
}

.offline_namebox {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgb(228, 228, 228);
}

.offline_namebox text {
  display: inline-block;
}

.offline_dis {
  width: 100%;
  padding: 26rpx 20rpx 0rpx;


}

.boxs {
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-radius: 6rpx;
  display: flex;
  margin-bottom: 20rpx;
}

.left_box_disabled {
  background: linear-gradient(to right, #dadada, #c5c5c5);
  height: 100%;
  width: 180rpx;
  border-right: 10rpx dotted #fff;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
  border-radius: 6rpx;
}

.left_box_disabled text:first-child {
  font-size: 32rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.left_box_disabled text:last-child {
  font-size: 20rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.offline_namebox .offline_name_disavled {
  display: inline-block;
  color: #aaa;
}

.shop_dis {
  width: 100%;
  padding: 0rpx 20rpx 0rpx;
  margin: 20rpx 0rpx;
  height: 77vh;
}

.shop_boxs {
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-radius: 6rpx;
  display: flex;
  margin-bottom: 20rpx;
}

.shop_left_box {
  background: linear-gradient(to right, #D8194F, #F35882);
  height: 100%;
  width: 600rpx;
  border-right: 10rpx dotted #fff;
  display: flex;
  justify-content: left;
  align-content: center;
  align-items: center;
  border-radius: 6rpx;
  color: #fff;
}

.shop_left_box .cny {
  font-size: 30rpx;
  color: #fff;
  line-height: 100rpx;
  height: 100rpx;
}

.shop_left_box .num {
  font-size: 120rpx;
  color: #fff;
  display: block;
  width: auto;
  height: 100rpx;
  overflow: hidden;
  text-align: left;
}

.date {
  height: 60rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx 0rpx 0rpx;
  justify-content: space-between;
}

.dateTime_disabled {
  font-size: 22rpx;
  color: #aaa;
}

.dateTime {
  font-size: 22rpx;
}

.gotoGet {
  font-size: 24rpx;
  color: #fff;
  display: block;
  width: 120rpx;
  border-radius: 10rpx;
  background: linear-gradient(to right, #ff592d, #ff2e40);
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 60rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 33.33%;
  color: #777777;
}

.on {
  color: #ff2e40;
  border-bottom: 4rpx solid #ff2e40;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.swiper-box view {
  text-align: center;
}

.shop_quan {
  font-size: 24rpx;
  display: block;
  width: 40rpx;
  height: 100rpx;
  border: 1rpx solid #fff;
  border-radius: 12rpx;
  margin-left: 20rpx;
}

.money_time {
  width: 300rpx;
  display: flex;
  color: #fff;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
}

.money_time text {
  width: 100%;
  font-size: 24rpx;
}

.shop_right_box {
  width: 110rpx;
  background: linear-gradient(to right, #ffc369, #FFAA00);
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.shop_right_box text {
  display: block;
  font-size: 26rpx;
  color: #fff;
  width: 30rpx;
  height: 120rpx;
}

.shop_left_box_disabled {
  background-color: rgb(230, 230, 230);
  height: 100%;
  width: 600rpx;
  border-right: 10rpx dotted #fff;
  display: flex;
  justify-content: left;
  align-content: center;
  align-items: center;
  border-radius: 6rpx;
  color: #aaa;
}

.shop_left_box_disabled .cny {
  font-size: 30rpx;
  color: #aaa;
  line-height: 100rpx;
  height: 100rpx;
}

.shop_left_box_disabled .num {
  font-size: 120rpx;
  color: #aaa;
  display: block;
  width: auto;
  height: 100rpx;
  overflow: hidden;
  text-align: left;
}

.shop_quan_disabled {
  font-size: 24rpx;
  display: block;
  width: 40rpx;
  height: 100rpx;

  border: 1rpx solid #aaa;
  border-radius: 12rpx;
  margin-left: 20rpx;
}

.money_time_disabled {
  width: 300rpx;
  display: flex;
  color: #aaa;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
}

.money_time_disabled text {
  width: 100%;
  font-size: 24rpx;
}

.shop_right_box_disabled {
  width: 110rpx;
  background: linear-gradient(to right, #d3d3d3, #8b8b8b);
