@import '/app.wxss';
@import '/wxParse/wxParse.wxss';
#reset {
  display: block;

}
#app{
  height: 100%;
}
page {
  height: 100%;
  
}
/* 2.主盒子 */
.contain {
  padding: 0rpx 20rpx;
  width: 100%;
  /* 宽度占屏幕的100% */
  height: 88vh;
  /* 高度占屏幕的100% */
  background-color: rgb(236, 236, 236);
  /* 背景颜色 */
  overflow-x: auto;
  overflow-y: scroll;
}
.detail_head{
  width: 100%;
  height: 120rpx;
  margin: 14rpx 0rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.detai_box{
  width: 100%;
background-color: #fff;
padding: 20rpx 30rpx 20rpx;
}
.wxParse-inline{
  font-size: 24rpx;
}

.head_box{
  width: 100%;
  height: 100%;
  padding: 20rpx 30rpx;
  
}
.head_box text:first-child{
  display: block;
  width: 100%;
  font-size: 24rpx;
}
.head_box text:last-child{
  display: block;
  width: 100%;
  font-size: 24rpx;
  line-height: 44rpx;
  border-bottom: 1rpx solid rgb(223, 223, 223);
}