/* pages/subOrder/subOrder.wxss */
@import '/app.wxss';

#reset {
  display: block;
  width: 100%;
  position: fixed;
  top: 0rpx;
}

page {
  height: 100%;
}

.contain {
  width: 100%;
  position: fixed;
  top: 10vh;
}

.useAddress {
  height: 194rpx;
  padding: 34rpx 24rpx 0rpx;
  position: relative;
  box-sizing: border-box;
}

.receive {
  display: block;
  width: 300rpx;
  color: #404040;
  font-size: 30rpx;
  line-height: 10rpx;
  position: absolute;
  top: 34rpx;
}

.phone {
  position: absolute;
  top: 30rpx;
  left: 300rpx;
  color: #999999;
  font-size: 28rpx;
}

.address {
  display: block;
  width: 646rpx;
  color: #404040;
  font-size: 24rpx;
  position: absolute;
  margin-top: 20rpx;
  top: 70rpx;
  line-height: 36rpx;
}

.goToAddress {
  display: block;
  position: absolute;
  left: 700rpx;
  top: 74rpx;
}

.border {
  display: block;
  width: 710rpx;
  height: 10rpx;
  position: absolute;
  bottom: 0rpx;

}
.addBtn{
  width:100% ;
  padding: 30rpx 26rpx;
}
.btn_add{
  width: 100%;
  height: 76rpx;
  text-align: center;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn_add text{
  font-size: 30rpx;
  color: #BFBFBF;
}
.sure_booter {
  display: flex;
  height: auto;
}

.choose_attr_view {
  padding: 30rpx 0;
  border-bottom: solid 1rpx #e6e6e6;
}

.timeBox {
  height: 92rpx;
  padding: 32rpx 28rpx;
  position: relative;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.title {

  font-size: 26rpx;
  color: #3A3A3A;

}

.time {

  width: 400rpx;
  font-size: 24rpx;
  color: #7A7979;
  position: absolute;
  left: 370rpx;
}

.timeIcon {
  position: absolute;
  left: 700rpx;
}

.typeBox {
  height: 92rpx;
  padding: 32rpx 28rpx;
  position: relative;
  box-sizing: border-box;
}

.groups {
  position: absolute;
  top: 30rpx;
  left: 404rpx;
  width: 300rpx;
  display: flex;
  justify-content: space-between;
}

van-radio {
  color: #7A7979;
  font-size: 24rpx;

}

.goodItem {

  position: relative;
}

.orderImage {
  width: 124rpx;
  height: 124rpx;
}

.name {
  text-overflow:ellipsis;
  z-index: -1;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  width: 306rpx;
  font-size: 28rpx;
  color: #323232;
  position: absolute;
  left: 146rpx;
  top: -30rpx;
  line-height: 34rpx;
}

.price {
  display: block;
  text-align: center;
  font-size: 26rpx;
  color: #FF2B41;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
}

.Specifications {
  font-size: 24rpx;
  color: #999999;
  position: absolute;
  left: 146rpx;
  top: 50rpx;
}

.number {
  font-size: 20rpx;
  color: #888888;
  position: absolute;
  top: 40rpx;
  right: 0rpx;
}

.goods {
  height: 550rpx;
  padding: 36rpx 26rpx;
}

.goodItem {
  margin-bottom: 36rpx;
}

.allnum {
  width: 100%;
  height: 50rpx;
  position: relative;
}

.text {
  color: #404040;
  font-size: 26rpx;
  position: absolute;
  right: 130rpx;
  line-height: 40rpx;
}

.allMoney {
  font-size: 30rpx;
  color: #FF2B41;
  font-weight: 600;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}

.weBox {
  width: 100%;
  position: fixed;
  bottom: 0rpx;
 
}
.payTypes {
  height: 82rpx;
  padding: 10rpx 28rpx;
  position: relative;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgb(226, 226, 226);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.payType {
  height: 92rpx;
  padding: 32rpx 28rpx;
  position: relative;
  box-sizing: border-box;
}
.title {

  font-size: 26rpx;
  color: #3A3A3A;

}
.money{
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.sureOrder_foot {
  width: 100%;
  height: 100rpx;
  padding: 0rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content:space-between;
}

.moneyAll text:first-child {
  color: #333333;
  font-size: 18rpx;
}
.moneyAll text:last-child {
  color: #FF2150;
  font-size: 26rpx;

}
.zhifu {
  height: 80rpx;
}

.offline_dis {
  width: 100%;
  padding: 26rpx 20rpx 0rpx;
  height: auto;
}

.boxs {
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-radius: 6rpx;
  display: flex;
  margin-bottom: 20rpx;
}

.left_box {
  background: linear-gradient(to right, #ff592d, #ff2e40);
  height: 100%;
  width: 180rpx;
  border-right: 10rpx dotted #fff;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
  border-radius: 6rpx;
}

.left_box text:first-child {
  font-size: 32rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.left_box text:last-child {
  font-size: 20rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}
.shop_namebox {
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgb(228, 228, 228);
}

.shop_namebox text {
  display: inline-block;
}
.shop_name{
  font-size: 22rpx;
}

.quan_title{
  width: 100%;
  height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #e9e9e9;
}
.quans_title{
  width: 100%;
  height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}
.offline_dis_name {
  width: 80%;
  padding-left: 20rpx;
}
.date {
  height: 60rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx 0rpx 0rpx;
  justify-content: space-between;
}


.dateTime {
  font-size: 24rpx;
}