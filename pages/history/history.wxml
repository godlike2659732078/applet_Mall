<view>
	<van-nav-bar id="reset" title="易货商品上传记录" bind:click-left="onClickLeft" custom-style="height:{{navH}}rpx; background:linear-gradient(to right, #ff592d,#ff2e40);  ">
		<view style="height:100rpx;display:flex;align-items:center" slot="left">
			<van-icon name="arrow-left" custom-class="icon " size="40rpx" />
		</view>
	</van-nav-bar>
	<view class="contain">
		<block wx:for="{{historyList}}" wx:key="index">
			<view class="hisBox">
				<view class="detail">
					<view class="image_box">
						<image class="image_box" src="{{item.picture1}}"></image>
					</view>
					<view class="detail_item">
						<text>商品名称：{{item.goodsName}} </text>
						<text> 零售价：{{item.constPrice}}</text>
						<text>成本价：{{item.price}}</text>
					</view>
					<view class="status">
						<text class="type">
        审核状态
        </text>
						<text wx:if="{{item.status==0}}" class="status_item">审核中</text>
						<text wx:elif="{{item.status==1}}" class="status_item">审核通过</text>
						<text wx:elif="{{item.status==2}}" class="status_item">审核拒绝</text>
						<text wx:elif="{{item.status==2}}" class="status_item">{{item.reason}}</text>
					</view>
				</view>
			</view>
		</block>
	</view>
</view>