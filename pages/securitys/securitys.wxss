@import '/app.wxss';

#reset {
  display: block;
  width: 100%;
  position: fixed;
  top: 0rpx;
}

page {
  height: 100%;
}

.contain {
  width: 100%;
  height: 100%;
  padding: 20rpx 38rpx 0rpx;
  position: fixed;
  top: 10%;
}

.items {
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.title {
  color: #333333;
  font-size: 28rpx;
}

.name {
  color: #999999;
  font-size: 28rpx;
}
.status{
display: flex;
align-items: center;
font-size: 24rpx;
}
