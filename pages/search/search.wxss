.box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.orderBox {
  width: 100%;
  height: auto;
  background: #fff;
  border-radius: 20rpx;
  padding:0rpx 26rpx;
  margin:  20rpx 0rpx;
  display: flex;

}

.boxs {
  width: 340rpx;
  height: 400rpx;
  margin-bottom: 2rpx;
  background-color: #f5f5f5;
  border-radius:20rpx;
  
}

.box_image {
  width: 100%;
  height: 260rpx;

}
.image_item{
  width: 90%;
  height: 260rpx;
  margin: 20rpx 18rpx 20rpx;
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}
.goodsList {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  flex-direction:row;
  box-sizing: border-box;
  padding: 0rpx 24rpx;
  width: 100%;
  height: 100%;
}
.box_items {
  width: 100%;
  height: auto;
  padding: 0rpx 26rpx;
}

.box_items .item_title {

  width: 100%;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  color: #626262;
  line-height: 30rpx;
  font-size: 24rpx;

}

.box_items .item_price {
  display: block;
  width: 98rpx;
  color: #FF0000;
  line-height: 30rpx;
  font-size: 26rpx;
  font-weight: 600;
  margin-top: 10rpx;
}
.orderall {
  position: relative;
}
.orderImage {
  width: 230rpx;
  height: 230rpx;
}
