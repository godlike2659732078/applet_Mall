page {
  height: 100%;
  background-color: #fff;
}
#reset{
  position: fixed;
  top: 0rpx;
  display: block;
  width: 100%;
}
/* 2.主盒子 */
.contain {
  padding: 0rpx;
  width: 100%;
  /* 宽度占屏幕的100% */
  height: 100%;
  /* 高度占屏幕的100% */
  background-color: #fff;
  position: fixed;
  top: 10%;
  /* 背景颜色 */
}
.hotBox{
  display: flex;
  align-items: center;
  justify-content: left;
  flex-wrap: wrap;
}
.hotBox text{
  width: auto;
  font-size: 24rpx;
  line-height: 0rpx;
  padding: 20rpx 26rpx;
  border: 1rpx solid rgb(218, 218, 218);
  margin-left: 30rpx;
  border-radius:10rpx ;
  font-size: 22rpx;
  background-color: rgb(240, 240, 240);
  margin-bottom: 20rpx;
}
.about_shop {
  display: block;
  width: 100%;
  margin-top: 40rpx;
  font-size: 26rpx;
  text-indent: 2em;
}


