/* pages/success/success.wxss */
@import '/app.wxss';
#reset{
display: block;
width: 100%;
  position: fixed;
  top: 0rpx;
}
page{
  height: 100%;
}
.tabs{
  width: 100%;
  position: fixed;
  top: 10%;
}
.contain{
width: 100%;
height: 100%;
/* position: fixed;
top: 10%; */
background-color: firebrick;
}
.box{
  width: 100%;
  height: auto;
  padding: 26rpx 40rpx;
  border-bottom: 1rpx solid rgb(218, 218, 218);
}
.title{
  display: flex;
  justify-content: space-between;
}
.title text:first-child{
  color: #333333;
  font-size: 28rpx;
  width: 500rpx;
}
.title text:last-child{
  color: #FF2B41;
  font-size: 28rpx;
}
.date{
  display: flex;
  justify-content: space-between;
  line-height: 48rpx;
}
.date text:first-child{
  color: #999999;
  font-size: 28rpx;
}
.date text:last-child{
  color: #999999;
  font-size: 24rpx;
}