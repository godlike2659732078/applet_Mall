/* pages/address/address.wxss */
/* pages/buySucess/buySucess.wxss */
/* pages/success/success.wxss */
@import '/app.wxss';

#reset {
  display: block;
  width: 100%;
  position: fixed;
  top: 0rpx;
}

page {
  height: 100%;
}

.contain {
  width: 100%;
  height: 100%;
  padding: 30rpx 0rpx 0rpx;
  position: fixed;
  top: 10%;
}

.addressBox {
  width: 100%;
  background-color: #fff;
  height: 324rpx;
  margin-bottom: 24rpx;
  padding: 0rpx 40rpx 0rpx;
  box-sizing: border-box;
  position: relative;
}

.addressBox image {
  width: 670rpx;
  height: 324rpx;
}

.audit_head {
  width: 670rpx;
  height: 100rpx;
  padding: 0rpx 58rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  top: 0rpx;
}

.main text{
  color: #333333;
  font-size: 30rpx;
}
.audit_head text:last-child {

  font-size: 28rpx;
}

.audit_box {
  width: 670rpx;
  height: 200rpx;
  padding: 42rpx 58rpx;
  position: absolute;
  top: 100rpx;

}

.details {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 15rpx;
}
.details text:first-child{
  color: #666666;
  font-size: 28rpx;
}
.details text:last-child{
  color: #333333;
  font-size: 28rpx;
}