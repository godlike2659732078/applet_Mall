/* pages/buySucess/buySucess.wxss */
/* pages/success/success.wxss */
@import '/app.wxss';

#reset {
  display: block;
  width: 100%;
  position: fixed;
  top: 0rpx;
}

page {
  height: 100%;

}

.contain {
  width: 100%;
  height: 100%;
  padding: 20rpx 38rpx 0rpx;
  position: fixed;
  top: 10%;
}
.headPortrait{
  height: 112rpx;
  display: flex; 
 align-items: center;
 justify-content: space-between;

}
.head_image{
  width: 86rpx;
  height: 86rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.img_box{
  display: flex;
  align-items: center;
}
.items{
  width: 100%;
  height: 88rpx;

  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item_box{
  width: 100%;
  height: 88rpx;
  padding-right: 6rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.title{
  color: #333333;
  font-size: 28rpx;
}
.name{
  color: #999999;
  font-size:28rpx ;
}
.footBox{
  margin-top: 20rpx;
}
.signOut{
  width: 100%;
  height: 76rpx;
  display: flex;
  justify-content: center;
  align-items: center;
margin-top: 100rpx;
}
.outBtn{
  display: block;
  width: 450rpx;
  height: 76rpx;
  text-align:center;
  color: #fff;
  line-height: 76rpx;
  font-size: 26rpx;
  border-radius: 38rpx;
  background: linear-gradient(to right,#FF5D2B,#FF2B41);
}
.setName{
  width: 400rpx;
  height: 20vh;
  text-align: center;
padding: 20rpx;
}
.nameipt{
  text-align: left;
  margin-top: 4vh;
  border-bottom: 1px solid #aaa;
}

