@import '/app.wxss';
@import '/wxParse/wxParse.wxss';
/* @import "/wxParse/wxParse.wxss"; */

#reset{
  display: block;
  width: 100%;
  position: fixed;
  top: 0rpx;
  z-index: 10;
}
page{
  height: 100%;
}
.contain{
  width: 100%;
  position: fixed;
  top: 10%;
  height: 100%;
  padding: 0rpx 20rpx;
  background-color: #fff;
}

.banner_list_view .index-bg .slide-navigator {
  width: 100%;
  height: 100%;
}

.banner_list_view .index-bg  .slide-image {
  display: block;
  width: 100%;
  height: 100%;
}

.banner_list_view .index-bg .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 5rpx;
}

.banner_list_view .index-bg swiper {
  width: 100%;
  height: 100%;
  position: relative;
}

.banner_list_view .index-bg {
  width: 100%;
  height: 100%;
  position: relative;
}
.banner_list_view {
  height: 400rpx;
  width: 100%;
}
.showOut {
  display: block;
  width: 100%;
  height: 30rpx;
}
.openHead {
  position: relative;
  padding: 24rpx 26rpx 19rpx;
  border-bottom: 1px solid #D7D7D7;
}
.shopText{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  background-color: #fff;
  border-bottom: 1rpx solid rgb(235, 235, 235);
  padding: 0rpx 20rpx;
}
.content{
  width: 100%;
  height: auto;
  padding: 10rpx 20rpx;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  background-color: #fff;
  border-bottom: 1rpx solid rgb(235, 235, 235);
}

.head_box {
  width: 100%;
  height: 102rpx;
  padding: 0rpx 40rpx;
}

.charge_num {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #E6E6E6;
}

.charge_num text {
  font-size: 26rpx;
}


.charge_input {
  width: 80%;
}

.charge_placeholder {
  font-size: 26rpx;
  color: #BFBFBF;
}

.charge_type {
  width: 100%;
  padding: 0rpx 40rpx;
}

.choose {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #E6E6E6;
}

.choose text {
  color: #333333;
  font-size: 28rpx;
}

.pay_type {
  width: 100%;
}

.wechat_pay {
  width: 100%;
  height: 116rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #E6E6E6;
}

.image_box {
  padding: 4rpx 6rpx 0rpx;
}

.image_box image {
  width: 62rpx;
  height: 48rpx;
}
.text_box {
  width: 430rpx;
  height: 80rpx;
  display: flex;
flex-wrap: wrap;

}
.text_box text:first-child{
  color: #333333;
  font-size: 28rpx;
}
.text_box text:last-child{
  color: #989898;
  font-size: 20rpx;
}
.charge_btn{
  width: 600rpx;
  height: 98rpx;
}
.charge_btns {
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: center;
  margin-top: 120rpx;
}
.charges_btns {
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: center;
  margin-top: 0rpx;
}
.charge_btn {
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 32rpx;
  display: block;
  width: 600rpx;
  height: 98rpx;
  border-radius: 48rpx;

}

.payType {
  height: 82rpx;
  padding: 10rpx 28rpx;
  position: relative;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgb(226, 226, 226);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.title {

  font-size: 26rpx;
  color: #3A3A3A;

}
.money{
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.groups {
  width: auto;
  display: flex;
  justify-content: space-between;
}

.offline_dis {
  width: 100%;
  padding: 26rpx 20rpx 0rpx;
  height: auto;
}

.boxs {
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-radius: 6rpx;
  display: flex;
  margin-bottom: 20rpx;
}

.left_box {
  background: linear-gradient(to right, #ff592d, #ff2e40);
  height: 100%;
  width: 180rpx;
  border-right: 10rpx dotted #fff;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
  border-radius: 6rpx;
}

.left_box text:first-child {
  font-size: 32rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.left_box text:last-child {
  font-size: 20rpx;
  color: #fff;
  display: block;
  width: 100%;
  text-align: center;
}

.offline_dis_name {
  width: 80%;
  padding-left: 20rpx;
}

.offline_namebox {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgb(228, 228, 228);
}

.offline_namebox text {
  display: inline-block;
}
.quan_title{
  width: 100%;
  height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #e9e9e9;
}
.quans_title{
  width: 100%;
  height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}
.date {
  height: 60rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx 0rpx 0rpx;
  justify-content: space-between;
}


.dateTime {
  font-size: 24rpx;
}