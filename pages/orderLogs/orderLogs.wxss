@import '/app.wxss';

#reset {
  display: block;
  width: 100%;
  position: fixed;
  top: 0rpx;
}

page {
  height: 100%;
}

.contain {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 10%;
  overflow-x: auto;
  overflow-y: scroll;
}

.head {
  width: 100%;
  height: 200rpx;
  background: linear-gradient(to right, #FF5D2B, #FF2B41);
  position: relative;
}

.head text:first-child {
  color: #fff;
  font-size: 32rpx;
  position: absolute;
  left: 30rpx;
  bottom: 80rpx;
}

.head .clock{
  color: #fff;
  font-size: 30rpx;
  position: absolute;
  left: 30rpx;
  top: 62px;

}
.order_right{
  width: 188rpx;
  height: 126rpx;
  float: right;
  margin-top: 40rpx;
  margin-right: 30rpx;
}
.logs{
  padding: 0rpx 50rpx;
}