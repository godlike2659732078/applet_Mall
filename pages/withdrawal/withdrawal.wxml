<!--pages/share/share.wxml-->
<view>
	<van-nav-bar id="reset" title="提现" bind:click-left="onClickLeft" custom-style="height:{{navH}}rpx; background:linear-gradient(to right, #ff592d,#ff2e40);  ">
		<view style="height:100rpx;display:flex;align-items:center" slot="left">
			<van-icon name="arrow-left" custom-class="icon " size="40rpx" />
		</view>
	</van-nav-bar>
	<view class="account_container">
	
			<view class="aliPayBox" bindtap="gotoAlipay">
				<image class="headImage" src="../../images/user/alipay.png"></image>
				<view class="userInfo">
					<text>{{aliPay.bankName}}</text>
					<text>{{aliPay.card}}</text>
				</view>
				<van-icon name="arrow" />
			</view>

		<view class="head_box">
			<text class="title">账户余额将提现至支付宝</text>
			<view class="box_ipt">
				<view class="moneyBox">
					<text class="money">￥</text>
					<input type="number" bindinput="getMoneys" name="moneys" placeholder="请输入提现金额"></input>
				</view>
				<view class="textBox">
					<text class="title">备注信息:</text>
					<input type="number" name="remark"  bindinput="getRemark" placeholder="请输入备注信息"></input>
				</view>
			</view>
			<text class="text">可提现金额：￥{{amount}}</text>
		</view>
		<view class="btnBox" bindtap="gotoWithrawal">
			<text class="contact">申请提现</text>
		</view>
		<view class="agreement_text">
			<text class="text">点击申请提现，即表示您已经同意</text>
			<text class="agreement" bindtap="gotoAgreement">《提现协议》</text>
		</view>
	
	</view>
</view>